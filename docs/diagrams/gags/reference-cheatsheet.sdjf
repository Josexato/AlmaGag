  "connections": [
    {
      "from": "dir_a1",
      "to": "dir_b1",
      "label": "forward",
      "direction": "forward",
      "routing": { "type": "straight" }
    },
    {
      "from": "dir_a2",
      "to": "dir_b2",
      "label": "backward",
      "direction": "backward",
      "routing": { "type": "straight" }
    },
    {
      "from": "dir_a3",
      "to": "dir_b3",
      "label": "bidirectional",
      "direction": "bidirectional",
      "routing": { "type": "straight" }
    },
    {
      "from": "dir_a4",
      "to": "dir_b4",
      "label": "none (sin flechas)",
      "direction": "none",
      "routing": { "type": "straight" }
    },

    {
      "from": "rt_a1",
      "to": "rt_b1",
      "label": "straight",
      "direction": "forward",
      "routing": { "type": "straight" }
    },
    {
      "from": "rt_a2",
      "to": "rt_b2",
      "label": "orthogonal",
      "direction": "forward",
      "routing": { "type": "orthogonal" }
    },
    {
      "from": "rt_a3",
      "to": "rt_b3",
      "label": "bezier 0.35",
      "direction": "forward",
      "routing": { "type": "bezier", "curvature": 0.35 }
    },
    {
      "from": "rt_selfloop",
      "to": "rt_selfloop",
      "label": "arc self-loop",
      "direction": "forward",
      "routing": { "type": "arc", "radius": 50, "side": "top" }
    },

    {
      "from": "cnt_full",
      "to": "cnt_border",
      "label": "conexion interna",
      "direction": "forward",
      "routing": { "type": "straight" }
    },

    {
      "from": "prop_normal",
      "to": "prop_hp",
      "label": "hp escala alto",
      "direction": "forward"
    },
    {
      "from": "prop_normal",
      "to": "prop_wp",
      "label": "wp escala ancho",
      "direction": "forward"
    },

    {
      "from": "ico_server",
      "to": "ico_cloud",
      "direction": "forward"
    },
    {
      "from": "ico_cloud",
      "to": "ico_building",
      "direction": "forward"
    },
    {
      "from": "ico_building",
      "to": "ico_firewall",
      "direction": "forward"
    },
    {
      "from": "ico_firewall",
      "to": "ico_unknown",
      "label": "tipo no registrado = BWT",
      "direction": "forward"
    },

    {
      "from": "title_types",
      "to": "title_directions",
      "label": "elementos + direcciones",
      "direction": "forward",
      "routing": { "type": "orthogonal" }
    },
    {
      "from": "title_directions",
      "to": "title_routing",
      "label": "direcciones + routing",
      "direction": "forward",
      "routing": { "type": "orthogonal" }
    },
    {
      "from": "title_routing",
      "to": "title_colors",
      "label": "routing + colores",
      "direction": "forward",
      "routing": { "type": "orthogonal" }
    },
    {
      "from": "title_colors",
      "to": "title_container",
      "label": "colores + contenedores",
      "direction": "forward",
      "routing": { "type": "orthogonal" }
    },
    {
      "from": "title_container",
      "to": "title_props",
      "label": "contenedores + props",
      "direction": "forward",
      "routing": { "type": "orthogonal" }
    },
    {
      "from": "title_props",
      "to": "title_canvas",
      "label": "props + canvas",
      "direction": "forward",
      "routing": { "type": "orthogonal" }
    }
  ]
}
