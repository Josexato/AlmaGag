      "color": "orange"
    },
    {
      "id": "ico_unknown",
      "label": "type: cualquier otro\n(BWT fallback)",
      "type": "database",
      "color": "pink"
    },

    {
      "id": "title_directions",
      "label": "DIRECCIONES\n(direction)",
