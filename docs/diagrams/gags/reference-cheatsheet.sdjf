      "type": "server",
      "color": "lightgreen"
    },
    {
      "id": "dir_a2",
      "label": "destino",
      "type": "server",
      "color": "lightgreen"
    },
    {
      "id": "dir_b2",
      "label": "backward\n(flecha<-circulo)",
      "type": "server",
      "color": "lightblue"
    },
    {
