      "id": "title_routing",
      "label": "TIPOS DE ROUTING\n(routing.type)",
      "type": "building",
      "color": "orange",
      "contains": [
        { "id": "rt_a1", "scope": "full" },
        { "id": "rt_b1", "scope": "full" },
        { "id": "rt_a2", "scope": "full" },
