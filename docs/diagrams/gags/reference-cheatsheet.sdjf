    {
      "id": "title_container",
      "label": "CONTENEDORES\n(contains + scope)",
      "type": "building",
      "color": "lavender",
      "contains": [
        { "id": "cnt_parent", "scope": "full" }
      ]
    },
    {
      "id": "cnt_parent",
      "label": "Contenedor padre\npadding:10 aspect_ratio:1.3",
      "type": "building",
      "color": "lightblue",
      "contains": [
        { "id": "cnt_full", "scope": "full" },
        { "id": "cnt_border", "scope": "border" }
      ]
    },
    {
      "id": "cnt_full",
      "label": "scope: full\n(dentro del borde)",
      "type": "server",
      "color": "lightgreen"
    },
    {
      "id": "cnt_border",
      "label": "scope: border\n(en el borde)",
      "type": "server",
      "color": "orange"
    },

    {
      "id": "title_props",
      "label": "PROPIEDADES DE ELEMENTOS\n(element props)",
      "type": "building",
      "color": "pink",
      "contains": [
        { "id": "prop_normal", "scope": "full" },
        { "id": "prop_hp", "scope": "full" },
        { "id": "prop_wp", "scope": "full" },
        { "id": "prop_priority", "scope": "full" }
      ]
    },
    {
      "id": "prop_normal",
      "label": "hp:1.0 wp:1.0\n(tamano base 80x50)",
