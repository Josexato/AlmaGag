        { "id": "dir_a4", "scope": "full" },
        { "id": "dir_b4", "scope": "full" }
      ]
    },
    {
      "id": "dir_a1",
      "label": "origen",
      "type": "server",
      "color": "lightblue"
    },
    {
      "id": "dir_b1",
