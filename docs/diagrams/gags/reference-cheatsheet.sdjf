{
  "canvas": {
    "width": 2200,
    "height": 1800
  },
  "elements": [
    {
      "id": "title_types",
      "label": "TIPOS DE ICONOS\n(type)",
      "type": "building",
      "color": "lightblue",
      "contains": [
        { "id": "ico_server", "scope": "full" },
        { "id": "ico_cloud", "scope": "full" },
        { "id": "ico_building", "scope": "full" },
        { "id": "ico_firewall", "scope": "full" },
        { "id": "ico_unknown", "scope": "full" }
      ]
    },
    {
      "id": "ico_server",
      "label": "type: server\n(rectangulo gradiente)",
      "type": "server",
      "color": "lightgreen"
    },
    {
      "id": "ico_cloud",
      "label": "type: cloud\n(elipse gradiente)",
      "type": "cloud",
      "color": "lightblue"
    },
    {
      "id": "ico_building",
      "label": "type: building\n(cubo 3D isometrico)",
