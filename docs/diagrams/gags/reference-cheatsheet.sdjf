    {
      "id": "prop_hp",
      "label": "hp: 1.5\n(alto x1.5 = 75px)",
      "type": "server",
      "color": "lightgreen",
      "hp": 1.5
    },
    {
      "id": "prop_wp",
      "label": "wp: 1.5\n(ancho x1.5 = 120px)",
      "type": "server",
      "color": "orange",
      "wp": 1.5
    },
    {
      "id": "prop_priority",
      "label": "label_priority:\nhigh|normal|low",
      "type": "server",
      "color": "gold"
    },

    {
      "id": "title_canvas",
      "label": "CANVAS\n(propiedades globales)",
      "type": "building",
      "color": "cyan",
      "contains": [
        { "id": "canvas_info", "scope": "full" }
      ]
    },
    {
      "id": "canvas_info",
      "label": "canvas.width: px\ncanvas.height: px\n(auto-expand si necesario)",
      "type": "cloud",
      "color": "lightblue"
    }
